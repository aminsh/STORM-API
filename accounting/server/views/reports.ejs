<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Reports</title>
    <link href="/client/content/bootstrap/bootstrap.min.css" rel="stylesheet"/>

</head>
<body>
<a class="btn btn-primary"
   href="/report/designer/create"
   target="_blank"
   role="button">Create Report</a>

<table class="table">
    <thead>
    <tr>
        <th>Name</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    <% reports.forEach(function(report){ %>
    <tr>
        <td><%=: report.fileName %></td>
        <td>
            <a class="btn btn-default"
               href="/report/designer/<%=: report.fileName %>/edit"
               target="_blank"
               role="button">Edit</a>
        </td>
    </tr>
    <% }) %>
    </tbody>
</table>


</body>
</html>
