<!DOCTYPE html>

<html ng-cloak>
<head>
    <meta name="viewport" content="width=device-width"/>
    <meta name="token" content="<%- token %>"/>
    <meta http-equiv="Content-Type" content="text/html" charset="utf-8"/>

    <title ng-bind="branch ? branch.name : 'STORM'">STORM</title>

    <link href="/public/css/acc.min.css?v=<%- version.app %>" rel="stylesheet"/>

    <base href="/acc/" >
</head>
<body class="rtls k-rtl ">
<shell>
    <div class="waiting-overley">
        <div class="start-waiting-block">
            <div style="margin-bottom: 30px">
                <img src="/public/images/logo.png" style="height: 100px">
            </div>
            <div class="progress-line"></div>
        </div>
    </div>

</shell>

</body>

</html>
<script>
    String.prototype.format = String.prototype.f = function () {
        var s = this,
            i = arguments.length;

        while (i--) {
            s = s.replace(new RegExp('\\{' + i + '\\}', 'gm'), arguments[i]);
        }
        return s;
    };
</script>

<script src="/public/js/acc.bundle.min.js?v=<%- version.app %>"></script>
<script src="/public/js/acc.template.bundle.js?v=<%- version.app %>"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/xlsx/0.7.7/xlsx.core.min.js" async></script>
<script src="https://rawgithub.com/eligrey/FileSaver.js/master/FileSaver.js" type="text/javascript"></script>

<script>

    localStorage.setItem('today', '<%- today %>');
    localStorage.setItem('translate', '<%- JSON.stringify(translation) %>');
    localStorage.setItem('currentUser', '<%- JSON.stringify(currentUser) %>');
    localStorage.setItem('reports', '<%- JSON.stringify(reports) %>');
    localStorage.setItem('env', '<%- env %>');

</script>

